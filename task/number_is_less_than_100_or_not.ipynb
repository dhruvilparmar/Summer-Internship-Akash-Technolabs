      "source": [
        "def CheckForLess(list1, val):\n",
        "  return(all(x < val for x in list1))\n",
        "\n",
        "list1 = [10, 20, 30, 40, 50]\n",
        "val = 100\n",
        "if (CheckForLess(list1, val)): print(\"Yes\")\n",
        "else: print(\"No\")"
      ]
